#include <bits/stdc++.h>
using namespace std;
#define ii int
#define ll long long int
#define fast_io ios::sync_with_stdio(false); cin.tie(nullptr);

void solve() {
    ii n;
    cin>>n;
    ii a;
    for(int i=0; i < n; i++) {
        cin>>a;
        int d= a/1000;
        int e= a%1000;
        int l=0,r=0;
       
        l += d%10; d /= 10;
        l += d%10; d /= 10;
        l += d%10;

        r += e%10; e /= 10;
        r += e%10; e /= 10;
        r += e%10;

        if(l==r) cout<<"YES"<<endl;
        else cout<<"NO"<<endl;
    }
}

int main() {
    fast_io
    solve();
    return 0;
}
