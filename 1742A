#include<iostream>
#include<algorithm>
using namespace std;
#define fast_io ios::sync_with_stdio(false); cin.tie(nullptr);

void solve()
{
    int n,a,b,c;
    cin>>n;
    for(int i = 0; i < n; i++){
        cin>>a>>b>>c;

        int big = max({a,b,c});

        int sum = a + b + c;
        sum = sum - big;

        if(sum == big){
            cout<<"YES"<<endl;
        }
        else{
            cout<<"NO"<<endl;
        }
    }
}

int main()
{
    fast_io
    solve();

    return 0;
}
