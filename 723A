#include <bits/stdc++.h>
using namespace std;

#define fast_io ios::sync_with_stdio(false); cin.tie(nullptr);

void solve() {
   int a[3];
    int sum=0,total=0;

    for (int i=0; i<3; i++) {
        cin>>a[i];
        
    }

    sort(a,a+3);

    for (int i=0; i<3; i++) {

        int diff= abs(a[i] - a[1]);
       total = diff+total;
    }
    cout<<total;

}

int main() {
    fast_io
    solve();
    return 0;
}
