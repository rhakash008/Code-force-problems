#include<iostream>
#include<algorithm>
using namespace std;
#define fast_io ios::sync_with_stdio(false); cin.tie(nullptr);

void solve()
{
    int n, k, l, c, d, p, nl, np;
    cin >> n >> k >> l >> c >> d >> p >> nl >> np;

    int total_drink = (k * l) / nl;   
    int total_lime  = (c * d);        
    int total_salt  = p / np;         

    int result = min({total_drink, total_lime, total_salt}) / n;
    cout << result << "\n";

}

int main()
{
    fast_io
    solve();

    return 0;
}
