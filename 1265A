#include <bits/stdc++.h>
using namespace std;

#define ll long long int
#define q int
#define ff for(int i = 0; i < n; i++)
#define fast_io ios::sync_with_stdio(false); cin.tie(nullptr);

void solve() {
    q n;
    cin >> n;
    ff {
        string s;
        cin >> s;

        
        for (int j = 0; j < (int)s.size(); j++) {
            if (s[j] == '?') {
                for (char c : {'a', 'b', 'c'}) {
                    if (j > 0 && s[j - 1] == c) continue;
                    if (j + 1 < (int)s.size() && s[j + 1] == c) continue;
                    s[j] = c;
                    break;
                }
            }
        }


        bool ok = true;
        for (int j = 1; j < (int)s.size(); j++) {
            if (s[j] == s[j - 1]) {
                ok = false;
                break;
            }
        }

        if (!ok) cout << -1 << "\n";
        else cout << s << "\n";
    }
}

int main() {
    fast_io
    solve();
    return 0;
}
